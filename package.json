{
  "name": "wireswag",
  "version": "1.0.0",
  "description": "Create wiremock files and mappping from swagger file",
  "main": ".bin/index.js",
  "bin": {
    "wireswag": ".bin/index.js"
  },
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "standard": "standard --fix",
    "wireswag": "nodemon --exec babel-node -- src/index.js",
    "start-mock-server": "json-server --watch ./db/mock.json -p 4000",
    "dev": "yarn run wireswag -s ./samples/swagger.yml -g localhost:4000",
    "clean": "rm -rf ./.bin",
    "build": "babel src -d ./.bin"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nadzir/swagger-wiremock-node-codegen.git"
  },
  "keywords": [
    "swagger",
    "wiremock",
    "stub",
    "mock"
  ],
  "author": "Nadzir",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nadzir/swagger-wiremock-node-codegen/issues"
  },
  "homepage": "https://github.com/nadzir/swagger-wiremock-node-codegen#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "jest": "^23.0.0",
    "parcel": "^1.8.1",
    "standard": "^11.0.1"
  },
  "dependencies": {
    "commander": "^2.15.1",
    "js-yaml": "^3.11.0",
    "json-server": "^0.12.2",
    "lodash": "^4.17.10",
    "superagent": "^3.8.3"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": [
      "/.bin/"
    ]
  }
}